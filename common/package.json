{
  "name": "common",
  "version": "1.0.0",
  "description": "",
  "exports": {
    "./lib/*": {
      "types": "./lib/*.ts",
      "default": "./lib/*.ts"
    },
    "./database": {
      "types": "./database/index.ts",
      "default": "./database/index.ts"
    },
    "./database/*": {
      "types": "./database/*.ts",
      "default": "./database/*.ts"
    },
    "./database/schema": {
      "types": "./database/schema/index.ts",
      "default": "./database/schema/index.ts"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
